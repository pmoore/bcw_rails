
/* Main Header */
.city-landing-header{
    height: 152px;
    background: transparent url('<%= 'bg-city-header.jpeg' %>') repeat-x 0 0;
}

.city-landing-logo{
    width: 600px;
    height: 131px;
    background-position: 0 20px;
    background-repeat: no-repeat;
    float: left;
}
.city-landing-bcw-logo {
    float: right;
    width: 234px;
    height: 138px;
    background-repeat: no-repeat;
    background-image: url('<%= asset_path 'BCW-label.gif' %>');
    padding-bottom: 14px;
}

/* City Specific Logos */
.city-landing-logo.dc{
    background-image: url('<%= asset_path 'city_logos/dc.gif' %>');
}